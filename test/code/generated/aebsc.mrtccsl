assume {
    
    duration : 0ms <= Camera_PERIOD_JITTER <= 0ms;
    continuous process Camera_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Camera_EXEC <= 1ms;
    continuous process Camera_EXEC with normal(1ms, 0ms);
        

    duration : 0ms <= Lidar_PERIOD_JITTER <= 0ms;
    continuous process Lidar_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Lidar_EXEC <= 1ms;
    continuous process Lidar_EXEC with normal(1ms, 0ms);
        

    duration : 0ms <= Radar_PERIOD_JITTER <= 0ms;
    continuous process Radar_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Radar_EXEC <= 1ms;
    continuous process Radar_EXEC with normal(1ms, 0ms);
        

    duration : 0ms <= Brake_PERIOD_JITTER <= 0ms;
    continuous process Brake_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Brake_EXEC <= 1ms;
    continuous process Brake_EXEC with normal(1ms, 0ms);
        

    duration : 1ms <= Alarm_EXEC <= 1ms;
    continuous process Alarm_EXEC with normal(1ms, 0ms);

    duration : 0ms <= Data_hub_Fuse_detection_PERIOD_JITTER <= 0ms;
    continuous process Data_hub_Fuse_detection_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Data_hub_Fuse_detection_EXEC <= 1ms;
    continuous process Data_hub_Fuse_detection_EXEC with normal(1ms, 0ms);
        

    duration : 0ms <= Data_hub_Fuse_camera_PERIOD_JITTER <= 0ms;
    continuous process Data_hub_Fuse_camera_PERIOD_JITTER with normal(0s, 0ms);
    
    duration : 1ms <= Data_hub_Fuse_camera_EXEC <= 1ms;
    continuous process Data_hub_Fuse_camera_EXEC with normal(1ms, 0ms);
        

    duration : 1ms <= AEB_controler_Compute_detection_EXEC <= 1ms;
    continuous process AEB_controler_Compute_detection_EXEC with normal(1ms, 0ms);

    duration : 1ms <= AEB_controler_Compute_reaction_EXEC <= 1ms;
    continuous process AEB_controler_Compute_reaction_EXEC with normal(1ms, 0ms);

} structure {
    
    Camera_START = periodic 5ms with jitter Camera_PERIOD_JITTER;
    Camera_FINISH = delay Camera_START by Camera_EXEC;
    
    Camera_FINISH = Camera;
        

    Lidar_START = periodic 5ms with jitter Lidar_PERIOD_JITTER;
    Lidar_FINISH = delay Lidar_START by Lidar_EXEC;
    
    Lidar_FINISH = Lidar;
        

    Radar_START = periodic 5ms with jitter Radar_PERIOD_JITTER;
    Radar_FINISH = delay Radar_START by Radar_EXEC;
    
    Radar_FINISH = Radar;
        

    Brake_START = periodic 5ms with jitter Brake_PERIOD_JITTER;
    Brake_FINISH = delay Brake_START by Brake_EXEC;
    
        

    Alarm_START = Alarm;
    Alarm_FINISH = delay Alarm_START by Alarm_EXEC;
    
            

    Data_hub_Fuse_detection_START = periodic 5ms with jitter Data_hub_Fuse_detection_PERIOD_JITTER;
    Data_hub_Fuse_detection_FINISH = delay Data_hub_Fuse_detection_START by Data_hub_Fuse_detection_EXEC;
    
    Data_hub_Fuse_detection_FINISH = Detection_data;
        

    Data_hub_Fuse_camera_START = periodic 5ms with jitter Data_hub_Fuse_camera_PERIOD_JITTER;
    Data_hub_Fuse_camera_FINISH = delay Data_hub_Fuse_camera_START by Data_hub_Fuse_camera_EXEC;
    
    Data_hub_Fuse_camera_FINISH = Alarm;
    Data_hub_Fuse_camera_FINISH = Reaction_data;
        

    AEB_controler_Compute_detection_START = Detection_data;
    AEB_controler_Compute_detection_FINISH = delay AEB_controler_Compute_detection_START by AEB_controler_Compute_detection_EXEC;
    
    AEB_controler_Compute_detection_FINISH = Processed_detection;
            

    AEB_controler_Compute_reaction_START = Reaction_data;
    AEB_controler_Compute_reaction_FINISH = delay AEB_controler_Compute_reaction_START by AEB_controler_Compute_reaction_EXEC;
    
    AEB_controler_Compute_reaction_FINISH = Alarm;
    AEB_controler_Compute_reaction_FINISH = Brake;
            

}