assume {
    var global : clock;
    continuous process j with normal(0s, 0.3s);
    global = periodic 5s with jitter j offset 2s;
} structure {
    sensor {
        start = skip 3 every 5 with jitter 0 of global;
        end = start $ 10 on global;
    };
    controller {
        start = sensor.end;
        end = start $ [15,20] on global;
    };
    actuator {
        start = sensor.end;
        end = start $ [10,15] on global;
    };
    mutex {
        sensor.start -> sensor.end,
        controller.start -> controller.end,
        actuator.start -> actuator.end,
    };
    o = fastest of a b c d;
    duration : i <= j;
    integer : 3 < 5;
    o = sample a on b;
    a1 = next a0;
    o = (a or b) and (c or d);
    o = skip 5 every 4 with jitter [-1,1] of global;
    a5 = a0 $ 5;
    o = sporadic 5s;
} assert {
    a precedes b;
    b precedes c;
}
